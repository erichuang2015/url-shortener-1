{
  "name": "url-shortener",
  "version": "1.0.0",
  "private": true,
  "description": "Веб-приложение для сжатия ссылок",
  "main": "source/app.js",
  "scripts": {
    "start": "DEBUG=url-short:* node source/app",
    "deploy": "mina -f deploy.rb deploy",
    "develop": "export NODE_ENV=development && DEBUG=url-short:* nodemon source/app",
    "test": "export NODE_ENV=testing && mocha --timeout 120000 test/*.test.js",
    "precoverage": "if [ ! -d 'reports' ]; then mkdir 'reports'; else rm -rf 'reports/coverage'; fi",
    "coverage": "export NODE_ENV=testing && istanbul cover _mocha",
    "postcoverage": "mv ./coverage ./reports/coverage",
    "mutate": "export NODE_ENV=testing && stryker run"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.8",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "pug": "^2.0.0-rc.3",
    "request": "^2.83.0",
    "require-dir": "^0.3.2",
    "shortid": "^2.2.8",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chance": "^1.0.11",
    "codeclimate-test-reporter": "^0.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mockgoose": "^7.3.3",
    "rewire": "^2.5.2",
    "stryker": "^0.13.0",
    "stryker-api": "^0.11.0",
    "stryker-html-reporter": "^0.10.1",
    "stryker-mocha-framework": "^0.6.1",
    "stryker-mocha-runner": "^0.9.1",
    "supertest": "^3.0.0",
    "supports-color": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxnuke/url-shortener.git"
  },
  "author": "Semyon Fomin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taxnuke/url-shortener/issues"
  },
  "homepage": "https://github.com/taxnuke/url-shortener#readme"
}
