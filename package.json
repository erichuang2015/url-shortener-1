{
  "name": "url-shortener",
  "version": "0.0.1",
  "private": true,
  "description": "Веб-приложение для сжатия ссылок",
  "main": "app.js",
  "scripts": {
    "start": "DEBUG=url-short:* node app",
    "deploy": "mina -f deploy.rb deploy",
    "develop": "export NODE_ENV=development && DEBUG=url-short:* nodemon app",
    "test": "export NODE_ENV=testing && mocha --timeout 120000 test/*.test.js",
    "coverage": "export NODE_ENV=testing && istanbul cover _mocha"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.8",
    "path": "^0.12.7",
    "pug": "^2.0.0-rc.3",
    "shortid": "^2.2.8",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chance": "^1.0.11",
    "mockgoose": "^7.3.3",
    "rewire": "^2.5.2",
    "supertest": "^3.0.0",
    "supports-color": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxnuke/url-shortener.git"
  },
  "author": "Semyon Fomin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taxnuke/url-shortener/issues"
  },
  "homepage": "https://github.com/taxnuke/url-shortener#readme"
}
