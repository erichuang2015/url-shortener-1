{
  "private": true,
  "main": "source/server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "DEBUG=url-short:* node source/server",
    "deploy": "git push heroku master",
    "predevelop": "mongod&",
    "develop": "pm2 restart 'URL-ShortenerAPI' || pm2 start",
    "postdevelop": "webpack-dev-server --inline --progress --config source/gui/build/webpack.dev.conf.js",
    "pretest": "eslint --ignore-path .eslintignore .",
    "test": "export NODE_ENV=testing && mocha --timeout 120000 test/*.test.js",
    "precoverage": "if [ ! -d 'reports' ]; then mkdir 'reports'; else rm -rf 'reports/coverage'; fi",
    "coverage": "export NODE_ENV=testing && istanbul cover _mocha",
    "postcoverage": "mv ./coverage ./reports/coverage",
    "premutate": "if [ ! -d 'reports' ]; then mkdir 'reports'; else rm -rf 'reports/mutation'; fi",
    "mutate": "export NODE_ENV=testing && stryker run",
    "show-coverage": "open ./reports/coverage/lcov-report/index.html",
    "show-msi": "open ./reports/mutation/html/index.html",
    "lint": "eslint --ext .js,.vue source/gui/src",
    "build": "node source/gui/build/build.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm t",
      "pre-push": "npm t"
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.8",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "pm2": "^3.2.2",
    "request": "^2.83.0",
    "require-dir": "^0.3.2",
    "shortid": "^2.2.8",
    "underscore": "^1.8.3",
    "axios": "^0.18.0",
    "vue": "^2.5.2",
    "vue-yandex-metrika": "^1.7.2",
    "vuetify": "^1.2.10"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.1.1",
    "chalk": "^2.0.1",
    "chance": "^1.0.11",
    "codeclimate-test-reporter": "^0.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mockgoose": "^7.3.3",
    "node-notifier": "^5.1.2",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stryker": "^0.29.2",
    "stryker-api": "^0.21.1",
    "stryker-html-reporter": "^0.16.2",
    "stryker-javascript-mutator": "^0.10.0",
    "stryker-mocha-framework": "^0.12.2",
    "stryker-mocha-runner": "^0.14.2",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
